{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@views/*": [
        "src/views/*"
      ],
      "@router/*": [
        "src/router/*"
      ]
    },
    "allowJs": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
